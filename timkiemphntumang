<script>
   var value=prompt("nhập 1 số: ");
   var x=[1,2,3,4,5,6];
   for(var i=0;i<x.length;i++){
       if(value==x[i]){
           alert("value" + x[i] + "in" + i);
       }else{
           alert("không tìm thấy");
       }
   }
</script>
